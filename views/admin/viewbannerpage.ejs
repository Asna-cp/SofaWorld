    <body>

      <div class="card mt-2 mx-4 ">
        <h5 class="card-header">All Banners</h5>

        <div class="table-responsive text-nowrap">
          <table class="table table-dark table-striped">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <!-- <th scope="col">Type</th>
                <th scope="col">Seating</th>
                <th scope="col">Quantity</th> -->
                <th scope="col">description</th>
                <!-- <th scope="col">Price</th> -->
                <th scope="col">Image</th>
                <th scope="col">status</th>
                <!-- <th scope="col">OPTIONS</th> -->
              </tr>
            </thead>
            <tbody>
              <% banners.forEach(function(product){ %>
                <% console.log(product) %>
                  <tr>
                    <th scope="row">
                      <%= index++ %>
                    </th>
                    <td>
                      <%= product?.bannerName %>
                    </td>
                    
                    <td>
                      <%= product?.discription %>
                    </td>
                    
                    <td><img src="/images/<%= product?.image %>" alt="" style="height: 50px; width: 60px;"></td>
                
                    <td>
                      <div>
                        <% if (product?.update==false) { %>
                          <form action="/admin/listBanner/<%= product?._id %>" method="post">
                            <button type="submit" class="btn btn-danger" style="width: 100px;">List</button>
                          </form>
                          <% } else { %>
                            <form action="/admin/unlistBanner/<%= product._id %>" method="post">
                              <button type="submit" class="btn btn-success" style="width: 100px;">UnList</button>
                            </form>


                            <% } %>

                              <br>
                          

                      </div>

                    </td>

                  </tr>
                  <% }) %>

            </tbody>
          </table>
        </div>
